{#
/**
 * @file
 * Default theme implementation for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 *
 * @ingroup themeable
 */
#}
<form{{ attributes.addClass('gs-form') }}>

  <fieldset>

    <legend {{ title_attributes.addClass('form-legend') }}> Inventaire </legend>
    
    <div class="row">
        {{ children }}
      </div>

  </fieldset>
  
</form>
